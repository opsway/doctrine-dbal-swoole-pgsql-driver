{
    "name": "opsway/doctrine-dbal-swoole-pgsql-driver",
    "description": "The PostgreSQL Swoole driver for Doctrine",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Vronskiy",
            "email": "alvro@opsway.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpsWay\\Doctrine\\DBAL\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "beberlei/assert": "^3.3",
        "doctrine/dbal": "^3.6"
    },
    "require-dev": {
        "openswoole/ide-helper": "dev-master",
        "opsway/psr12-strict-coding-standard": "^1.0",
        "vimeo/psalm": "^5.00",
        "weirdan/doctrine-psalm-plugin": "^2.0"
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
